s1=0
s2=0
k1=0
k2=0
for x in range(12):
    n=int(input("Введите число "))
    if n%2==0:
        s1=s1+n
        k1=k1+1
    else:
        s2=s2+n
        k2=k2+1
if k1>0:
    a1=s1/k1
    print("Среднее арифметическое четных чисел ", a1)
else:
    print("Четных чисел нет ")
if k2>0:
    a2=s2/k2
    print("Среднее арифметическое нечетных чисел ", a2)
else:
    print("Нетных чисел нет ")
    
