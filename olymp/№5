k1=int(input("Введите первую координату первой клетки "))
k2=int(input("Введите вторую координату первой клетки "))
m1=int(input("Введите первую координату второй клетки "))
m2=int(input("Введите вторую координату второй клетки "))
if k1%2==0 and k2%2==0 or k1%2==1 and k2%2==1:
    g1=0
else:
    g1=1
if m1%2==0 and m2%2==0 or m1%2==1 and m2%2==1:
    g2=0
else:
    g2=1
if g1==g2:
    print("YES")
else:
    print("NO")
