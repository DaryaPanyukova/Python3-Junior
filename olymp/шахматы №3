x1, y1, x2, y2=map(int, input().split())
if x1==x2 or y1==y2:
    print("Ферзь бьет коня")
elif abs(x1-x2)==abs(y1-y2):
    print("Ферзь бьет коня")
elif (abs(x2-x1)==1 and abs(y2-y1)==2) or (abs(x2-x1)==2 and abs(y2-y1)==1):
    print("Конь бьет ферзя")
else:
    print("Фигуры не угражают друг другу")

