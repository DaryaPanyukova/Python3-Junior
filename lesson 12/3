sl={"1а":"25","1б":"27","2а":"26","2б":"28","3а":"23","3б":"25","4а":"27","4б":"24","5а":"23","5б":"24"}
summa=0
sl["1а"]="23"
sl["3а"]="25"
sl["2б"]="29"
sl["3в"]="23"
sl["6а"]="24"
sl.pop("1б")


for s in sl:

    summa=int(sl[s])+summa
print("Общее кол-во учащихся - ",summa)
for s in sl:
    print(s,sl[s],sep=" 		")
