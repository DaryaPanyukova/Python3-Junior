N=int(input("Введите количество человек в совете "))
K=int(input("Введите количество родителей в совете "))
c=N/K
f=N/3
if N<0 or N<K<0:
    N=int(input("Вы ошиблись, попробуйте снова.Введите количество человек в совете "))
    K=int(input("Введите количество родителей в совете "))
if c>3:
    print(f-K)
else:
    print(0)
