from tkinter import *
root=Tk()
root.title("Пример")
root.geometry("500x400+500+200")
root.resizable(False,False)
but1=Button(root,text="Кнопка 1")
but2=Button(root, text="Кнопка 2")
but3=Button(root,text="Кнопка 3")
but4=Button(root, text="Кнопка 4")
but1.place(x=50, y=20)
but2.place(x=60, y=50)
but3.place(x=70, y=70)
but4.place(x=80, y=90)
root.mainloop()
